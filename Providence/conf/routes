# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
#Authentication controllers
GET    /users/login                @controllers.auth.AuthenticationController.loginUser()

#Landing Controller

GET     /                           @controllers.landing.LandingController.landing()
# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)




#swagger-ui
GET      /swagger                   @controllers.Application.swagger
GET     /api-docs                   @controllers.ApiHelpController.getResources
GET     /api-docs/users             @controllers.ApiHelpController.getResource(path = "/users")
